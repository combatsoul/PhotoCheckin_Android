	package com.example.photocheckin;

import android.os.Bundle;
import android.app.Activity;
import android.view.Menu;

public class IntroMain extends Activity {

    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.intro_main);
        
        //call btn register
        Button call_register = (Button) findViewById(R.id.register_btn);
        call_register.setOnClickListener(this);
    }


    @Override
    public boolean onCreateOptionsMenu(Menu menu) {
        // Inflate the menu; this adds items to the action bar if it is present.
        getMenuInflater().inflate(R.menu.intro_main, menu);
        return true;
    }
    
}
